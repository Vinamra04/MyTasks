{
  "expo": {
    "name": "My Tasks",
    "slug": "my-tasks-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#1e1e2f"
    },
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/logo.png",
      "bundleIdentifier": "com.vinamra.mytasks"
    },
    "android": {
      "package": "com.vinamra.mytasks",
      "icon": "./assets/logo.png",
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/logo.png"
    },
    "plugins": [
      "expo-notifications"
    ]
  }
}
